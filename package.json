{
  "name": "data-server",
  "version": "0.0.1",
  "description": "personal data server",
  "main": "server.js",
  "scripts": {
    "build": "tsc",
    "dev": "./node_modules/nodemon/bin/nodemon.js --exec ts-node src/server.ts",
    "start": "node ./dist/server.js",
    "prod": "npm run build && npm run start",
    "test": "jest"
  },
  "author": "Zijin Luo",
  "license": "Apache-2.0",
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "testMatch": [
      "**/test/**/*.test.(ts|tsx|js|jsx)"
    ],
    "testEnvironment": "node"
  },
  "dependencies": {
    "@types/jsonwebtoken": "^8.3.2",
    "body-parser": "^1.18.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^7.0.0",
    "express": "^4.16.4",
    "express-graphql": "^0.7.1",
    "graphql": "^14.2.1",
    "jsonwebtoken": "^8.5.1"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/compression": "0.0.36",
    "@types/cors": "^2.8.4",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.16.1",
    "@types/express-graphql": "^0.6.2",
    "@types/jest": "^24.0.11",
    "@types/node": "^11.13.0",
    "axios": "^0.18.0",
    "jest": "^24.7.0",
    "nodemon": "^1.18.10",
    "ts-jest": "^24.0.1",
    "ts-node": "^8.0.3",
    "tslint": "^5.15.0",
    "typescript": "^3.4.1"
  }
}
